# Local Development Environment
ENV=local

# Database Configuration (used by docker-compose)
POSTGRES_USER=dev
POSTGRES_PASSWORD=dev
POSTGRES_DB=biplace_local
POSTGRES_PORT=5432

# Backend Configuration (passed to backend app)
DATABASE_URL=postgresql://dev:dev@localhost:5432/biplace_local
NODE_ENV=development
PORT=3001

# Development specific
JWT_SECRET=local-jwt-secret-not-secure
LOG_LEVEL=debug