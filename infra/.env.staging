# Staging Environment
ENV=staging

# Database Configuration (used by docker-compose)
POSTGRES_USER=staging_user
POSTGRES_PASSWORD=staging_secure_password_change_me
POSTGRES_DB=biplace_staging
POSTGRES_PORT=5432

# Docker/Caddy Configuration (used by docker-compose)
BACKEND_PORT=3001
HTTP_PORT=80
HTTPS_PORT=443

# Backend Configuration (passed to backend app)
DATABASE_URL=postgresql://staging_user:staging_secure_password_change_me@postgres:5432/biplace_staging
NODE_ENV=staging
PORT=3001
JWT_SECRET=staging-jwt-secret-change-me-to-something-secure
LOG_LEVEL=info